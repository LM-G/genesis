{
  "name": "genesis-client",
  "version": "1.0.0",
  "description": "Genesis client",
  "main": "main.ts",
  "scripts": {
    "clean": "rimraf node_modules dist && npm cache clean",
    "clean-install": "npm run clean && npm install",
    "clean-start": "npm run clean-install && npm start",
    "server": "rimraf dist && webpack-dev-server --progress --env.dev --port 8080",
    "server:pretty": "webpack-dashboard -- webpack-dev-server --inline --progress --env.dev --port 8080",
    "build": "http-server dist --cors",
    "lint": "tslint --force \"src/**/*.ts\"",
    "test": "karma start",
    "test-watch": "karma start --no-single-run --auto-watch",
    "start": "npm run server",
    "start:hmr": "npm run server -- --hot"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LM-G/genesis.git"
  },
